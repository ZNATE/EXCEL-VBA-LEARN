Sub 粘贴1()
'
' 按钮
    ActiveSheet.Buttons.Add(585.5, 212, 50.5, 146.5).Select
    Selection.OnAction = "粘贴"
    Selection.Characters.Text = "粘贴"
    With Selection.Characters(Start:=1, Length:=2).Font
        .Name = "等线"
        .FontStyle = "常规"
        .Size = 11
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .ColorIndex = 1
    End With
    Range("AR20").Select
End Sub
