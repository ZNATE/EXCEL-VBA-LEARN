    Range("A5:Q6").Select
End Sub
Sub 宏16()
'
' 宏16 宏
'

'
    Range("B5:B" & XYZ + 4).Select
    With Selection.Font
        .ThemeColor = xlThemeColorLight1
        .TintAndShade = 0
    End With
End Sub
